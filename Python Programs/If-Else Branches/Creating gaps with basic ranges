movie_ticket_price = None

#Asks the user for their age
user_age = int(input('Enter your age: '))

if user_age <= 12:     # Age 12 and under prints "Child ticket discount" and sets the ticket price to 11
   print('Child ticket discount.')
   movie_ticket_price = 11
elif user_age >= 65:   # Age 65 and older prints "Senior ticket discount" and sets the ticket price to 12
   print('Senior ticket discount.') 
   movie_ticket_price = 12
else:                  # All other ages, sets ticket price to 14
   movie_ticket_price = 14

print(f'Movie ticket price: {movie_ticket_price}')