user_choice = int(input("Pick 1 or 2: "))
num_items = int(input("Enter a number: "))

if user_choice == 1:
    print("User choice is 1")
elif user_choice == 2:
    if num_items < 0:
        print("User choice is 2 and number of items is less than 0.")
    else:
        print("User choice is 2 and number of items is greater than or equal to 0.")
else:
    print("User choice is neither 1 or 2")
